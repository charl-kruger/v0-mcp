/**
 * Example Wrangler configuration for V0 MCP Server
 * Copy this file to wrangler.jsonc and customize for your deployment
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "v0-mcp",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-10",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "your-custom-domain.com",
      "custom_domain": true
    }
  ],
  "migrations": [
    {
      "new_sqlite_classes": ["MyMCP"],
      "tag": "v1"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "MyMCP",
        "name": "MCP_OBJECT"
      }
    ]
  },
  "observability": {
    "enabled": true
  }
} 